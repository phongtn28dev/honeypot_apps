{
  "github": {
    "enabled": false,
    "silent": true
  },
  "build": {
    "env": {
      "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org/"
    }
  },
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "ulimits": {
        "nofile": 65535
      }
    }
  }
}
